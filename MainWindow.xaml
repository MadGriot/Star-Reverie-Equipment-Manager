<Window x:Class="Star_Reverie_Inventory_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Star_Reverie_Inventory_Manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300
        ">
    <StackPanel Margin="10">
        <Button Content="Create Weapon" Margin="5"
                Click="CreateWeaponButton_Click"/>
        <Button Content="Display Weapons" Margin="5"
                Click="DisplayWeaponsButton_Click"/>
    </StackPanel>
</Window>
